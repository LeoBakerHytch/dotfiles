# Attributes: 00=none 01=bold 04=underscore 05=blink 07=reverse 08=concealed
# Text:       30=black 31=red 32=green 33=yellow 34=blue 35=magenta 36=cyan 37=white
# Background: 40=black 41=red 42=green 43=yellow 44=blue 45=magenta 46=cyan 47=white

RESET 0                     # Reset to 'normal' colour
EXEC 01;32                  # File with execute permission
DIR 01;34                   # Directory
LINK 01;36                  # Symbolic link
MULTIHARDLINK 00            # Regular file with more than one link
FIFO 40;33                  # Pipe
SOCK 01;35                  # Socket
DOOR 01;35                  # Door
BLK 40;33;01                # Block device driver
CHR 40;33;01                # Character device driver
ORPHAN 40;31;01             # Symlink to nonexistent file, or non-stat'able file
SETUID 37;41                # File that is setuid (u+s)
SETGID 30;43                # File that is setgid (g+s)
CAPABILITY 30;41            # File with capability
OTHER_WRITABLE 34;42        # Directory that is other-writable (o+w)
STICKY 37;44                # Directory with the sticky bit set (+t)
STICKY_OTHER_WRITABLE 30;42 # Directory that is sticky and other-writable (+t,o+w)

